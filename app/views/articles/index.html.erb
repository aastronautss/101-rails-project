<h1>All Articles</h1>

<p>
	<%= link_to "New Article", new_article_path, class: "new_article" %>
</p>

<p><%= link_to "Tags", tags_path %></p><br />

<ul id="articles">
	<% @articles.each do |article| %>
		<li>
			<%= link_to article.title, article_path(article), class: 'article_title', id: "article_#{article.id}" %>
		</li>
	<% end %>
</ul>
